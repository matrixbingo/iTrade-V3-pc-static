{
    "env":{
        "es6": false,
        "browser": true,
        "commonjs": true
    },
    "parser": "esprima",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": false,
            "modules": true,
            "impliedStrict": true,
            "allowImportExportEverywhere": false
        }
    },
    "globals":{
        "mqq": true,
        "KNB": true,
        "MTNB": true,
        "WebViewJavascriptBridge": true,
        "__VERSION__": true,
        "_MTNB": true
    },
    "rules": {
        "strict": ["error"],
        "quotes": [0, "single"],
        "no-use-before-define": ["error"],
        "no-console": [0],
        "no-unused-vars": ["warn"]
    },
    "extends": "eslint:recommended"
}
